<%= form_with model: @event do |f|%>
<%= f.hidden_field(:client_id, :value=> 1) %>
<%= f.label "Event Title" %>
<%= f.text_field :title %><br>
<%= f.label "Occasion" %>
<%= f.text_field :event_type %><br>
<%= f.label "Venue" %>
<%= f.collection_select :venue_id, Venue.all, :id, :name, include_blank: true %><br>
<%= f.label :date %>
<%= f.date_select(:date) %><br>
<%= f.submit %>
<% end %>
